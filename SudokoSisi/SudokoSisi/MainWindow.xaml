                        <ComboBoxItem  FontSize="20" FontWeight="Bold" Content="Medium"  Foreground="#126E7F" Background="#E8F3F1"/>
                        <ComboBoxItem   FontSize="20" FontWeight="Bold" Content="Difficult"  Foreground="#126E7F" Background="#E8F3F1"/>
                    </ComboBox>
                </MenuItem>
            </Menu>

